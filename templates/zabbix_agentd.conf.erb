Server=<%= $server_host %>
ServerPort=<%= $server_port %>
Hostname=<%= fqdn %>
StartAgents=<%= $num_agents %>
#RefreshActiveChecks=120
#DisableActive=1
DebugLevel=<%= $debug_level %>
PidFile=<%= $pidfile %>
LogFile=<%= $logfile %>
Timeout=3
<% if $agent_listen -%>
ListenPort=<%= $agent_listen_port %>
ListenIP=<%= $agent_listen_ip %>
<% end -%>
<% if $remote_cmd -%>
EnableRemoteCommands=1
<% end -%>