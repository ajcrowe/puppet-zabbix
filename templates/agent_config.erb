Server=<%= @server_host %>
ServerPort=<%= @server_port %>
Hostname=<%= @fqdn %>
StartAgents=<%= @num_agents %>
DebugLevel=<%= @debug_level %>
PidFile=<%= @pid_file %>
LogFile=<%= @log_file %>
Timeout=3
<% if $agent_listen -%>
ListenPort=<%= @listen_port %>
ListenIP=<%= @listen_ip %>
<% end -%>
<% if @remote_cmd -%>
EnableRemoteCommands=1
<% end -%>
<% if @custom_params -%>
<% @custom_params.each do |param| -%>
UserParameter=<%= @param %>
<% end -%>
<% end -%>